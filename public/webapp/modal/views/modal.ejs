<div class="modal hide fade" id="mod<%= id %>">
	<div class="modal-header">
		<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
		<h3><%= title %></h3>
	</div>
	<div class="modal-body" id="modalBody" <%= plugin(plug, { 'id' : id , 'content' : content } ) %>>
		
	</div>
</div>